{
  "name": "functions",
  "scripts": {
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start",
    "serveStord": "npm run build && firebase emulators:start --import ./data",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "pubsub": "npm run build &&  firebase emulators:start --only pubsub",
    "export": "firebase emulators:export ./emulator-data",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "functions": "npm run build && firebase emulators:start --only functions",
    "firestore": "firebase emulators:start --only firestore --import=./emulator-data",
    "watch": "nodemon -e ts --exec \"npm run functions\"",
    "postbuild": "cpy ./src/storage/** ./lib/storage/",
    "lint": "eslint --fix --ext .js,.jsx ."
  },
  "engines": {
    "node": "18"
  },
  "main": "lib/index.js",
  "dependencies": {
    "agora-access-token": "^2.0.4",
    "axios": "1.10.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "firebase": "^9.8.4",
    "firebase-admin": "^9.4.1",
    "firebase-functions": "^3.11.0",
    "i18n": "^0.14.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "nodemailer": "^6.7.5",
    "save": "^2.9.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/moment": "^2.13.0",
    "@types/node": "^18.7.13",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "cpy-cli": "^4.1.0",
    "eslint": "^8.18.0",
    "firebase-functions-test": "^0.2.0",
    "typescript": "^3.8.0"
  },
  "private": true
}
